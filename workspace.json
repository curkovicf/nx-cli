{
  "version": 2,
  "projects": {
    "nx-cli-angular-nx-cli": {
      "projectType": "application",
      "root": "apps/nx-cli/angular/nx-cli",
      "sourceRoot": "apps/nx-cli/angular/nx-cli/src",
      "prefix": "dev-workspace",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/nx-cli/angular/nx-cli",
            "index": "apps/nx-cli/angular/nx-cli/src/index.html",
            "main": "apps/nx-cli/angular/nx-cli/src/main.ts",
            "polyfills": "apps/nx-cli/angular/nx-cli/src/polyfills.ts",
            "tsConfig": "apps/nx-cli/angular/nx-cli/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/nx-cli/angular/nx-cli/src/favicon.ico",
              "apps/nx-cli/angular/nx-cli/src/assets"
            ],
            "styles": ["apps/nx-cli/angular/nx-cli/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/nx-cli/angular/nx-cli/src/environments/environment.ts",
                  "with": "apps/nx-cli/angular/nx-cli/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "nx-cli-angular-nx-cli:build:production"
            },
            "development": {
              "browserTarget": "nx-cli-angular-nx-cli:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "nx-cli-angular-nx-cli:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/nx-cli/angular/nx-cli/src/**/*.ts",
              "apps/nx-cli/angular/nx-cli/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/nx-cli/angular/nx-cli"],
          "options": {
            "jestConfig": "apps/nx-cli/angular/nx-cli/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nx-cli-angular-nx-cli-e2e": {
      "root": "apps/nx-cli/angular/nx-cli-e2e",
      "sourceRoot": "apps/nx-cli/angular/nx-cli-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/nx-cli/angular/nx-cli-e2e/cypress.json",
            "tsConfig": "apps/nx-cli/angular/nx-cli-e2e/tsconfig.e2e.json",
            "devServerTarget": "nx-cli-angular-nx-cli:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "nx-cli-angular-nx-cli:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/nx-cli/angular/nx-cli-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "nx-cli-angular-shell": {
      "projectType": "library",
      "root": "libs/nx-cli/angular/shell",
      "sourceRoot": "libs/nx-cli/angular/shell/src",
      "prefix": "dev-workspace",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/nx-cli/angular/shell"],
          "options": {
            "jestConfig": "libs/nx-cli/angular/shell/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/nx-cli/angular/shell/src/**/*.ts",
              "libs/nx-cli/angular/shell/src/**/*.html"
            ]
          }
        }
      }
    },
    "nx-cli-electron-nx-cli": {
      "root": "apps/nx-cli/electron/nx-cli",
      "sourceRoot": "apps/nx-cli/electron/nx-cli/src",
      "projectType": "application",
      "prefix": "nx-cli-electron-nx-cli",
      "targets": {
        "build": {
          "executor": "nx-electron:build",
          "options": {
            "outputPath": "dist/apps/nx-cli/electron/nx-cli",
            "main": "apps/nx-cli/electron/nx-cli/src/main.ts",
            "tsConfig": "apps/nx-cli/electron/nx-cli/tsconfig.app.json",
            "assets": ["apps/nx-cli/electron/nx-cli/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/nx-cli/electron/nx-cli/src/environments/environment.ts",
                  "with": "apps/nx-cli/electron/nx-cli/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "nx-electron:execute",
          "options": {
            "buildTarget": "nx-cli-electron-nx-cli:build"
          }
        },
        "package": {
          "executor": "nx-electron:package",
          "options": {
            "name": "nx-cli-electron-nx-cli",
            "frontendProject": "nx-cli",
            "outputPath": "dist/packages",
            "prepackageOnly": true
          }
        },
        "make": {
          "executor": "nx-electron:make",
          "options": {
            "name": "nx-cli-electron-nx-cli",
            "frontendProject": "nx-cli",
            "outputPath": "dist/executables"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/nx-cli/electron/nx-cli/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/nx-cli/electron/nx-cli"],
          "options": {
            "jestConfig": "apps/nx-cli/electron/nx-cli/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nx-cli-electron-shell": {
      "root": "libs/nx-cli/electron/shell",
      "sourceRoot": "libs/nx-cli/electron/shell/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/nx-cli/electron/shell/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/nx-cli/electron/shell"],
          "options": {
            "jestConfig": "libs/nx-cli/electron/shell/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nx-cli-electron-util-event-handlers": {
      "root": "libs/nx-cli/electron/util-event-handlers",
      "sourceRoot": "libs/nx-cli/electron/util-event-handlers/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/nx-cli/electron/util-event-handlers/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/nx-cli/electron/util-event-handlers"],
          "options": {
            "jestConfig": "libs/nx-cli/electron/util-event-handlers/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nx-cli-shared-data-events": {
      "root": "libs/nx-cli/shared/data-events",
      "sourceRoot": "libs/nx-cli/shared/data-events/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/nx-cli/shared/data-events/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/nx-cli/shared/data-events"],
          "options": {
            "jestConfig": "libs/nx-cli/shared/data-events/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-angular-material-util-material": {
      "projectType": "library",
      "root": "libs/shared/angular/material/util-material",
      "sourceRoot": "libs/shared/angular/material/util-material/src",
      "prefix": "dev-workspace",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/angular/material/util-material"],
          "options": {
            "jestConfig": "libs/shared/angular/material/util-material/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/angular/material/util-material/src/**/*.ts",
              "libs/shared/angular/material/util-material/src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "nx-cli-electron-nx-cli"
}
